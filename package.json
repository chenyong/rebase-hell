{
  "name": "@jimengio/rebase-hell",
  "version": "0.1.3",
  "description": "Cumulo Workflow",
  "main": "index.js",
  "bin": {
    "rebase-hell": "./dist/server.js"
  },
  "scripts": {
    "watch": "shadow-cljs watch client server",
    "build-cdn": "shadow-cljs clj-run build.main/build-cdn",
    "build": "shadow-cljs clj-run build.main/build",
    "page": "shadow-cljs clj-run build.main/page",
    "upload": "lumo -c cli/:src/ -m build.upload",
    "up": "yarn build-cdn && yarn upload",
    "pkg": "rm -rf dist/* && yarn shadow-cljs release server",
    "serve": "http-server dist -s",
    "repl": "rlwrap shadow-cljs clj-repl client",
    "repl-server": "rlwrap shadow-cljs clj-repl server",
    "ln": "cd target && rm -f entry && ln -s ../entry",
    "prod": "yarn install --production"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:chenyong/rebase-hell.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "latest-version": "^4.0.0",
    "md5": "^2.2.1",
    "randomcolor": "^0.5.3",
    "shortid": "^2.2.14",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "feather-icons": "^4.9.0",
    "http-server": "^0.11.1",
    "shadow-cljs": "^2.7.9",
    "source-map-support": "^0.5.9"
  }
}
